<T3DataStructure>
<meta>
	<langDisable>1</langDisable>
</meta>
<sheets>
	<sDEF>
		<ROOT>
			<TCEforms>
				<sheetTitle>Settings</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<!-- Plugin action -->
				<switchableControllerActions>
					<TCEforms>
						<label>LLL:EXT:netc_bakerlist/Resources/Private/Language/locallang.xlf:addresses.selectaction</label>
						<onChange>reload</onChange>
						<config>
							<type>select</type>
							<items>
								<numIndex index="1">
									<numIndex index="0">LLL:EXT:netc_bakerlist/Resources/Private/Language/locallang.xlf:addresses.BakerlistSearch</numIndex>
									<numIndex index="1">Addresses->list;Addresses->show;</numIndex>
								</numIndex>
							</items>
						</config>
					</TCEforms>
				</switchableControllerActions>
				<!-- View -->
				<settings.storage_pid>
					<TCEforms>
						<label>LLL:EXT:netc_bakerlist/Resources/Private/Language/locallang.xlf:addresses.storagefolder</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</settings.storage_pid>
				<settings.Glutenfreie>
					<TCEforms>
						<label>LLL:EXT:netc_bakerlist/Resources/Private/Language/locallang.xlf:addresses.Glutenfreie</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</settings.Glutenfreie>
			</el>
		</ROOT>
	</sDEF>
</sheets>
</T3DataStructure>
